{
    "namespace": "animations_ui",

    /* Base */
    "anim_uv_base":
    {
        "anim_type": "uv",
        "next": "$next_frame", 
        "from": "$uv_frame",
        "to": "$uv_frame",
        "duration": 0.1
    },

    /* Anvil Hammer */
    "anvil_hammer":
    {
        "layer": 2,
        "type": "image",
        "size": [38,38],
        "offset": "$anvil_hammer_offset",
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "uv":
        {
            "anim_type": "flip_book",
            "initial_uv": [0,0],
            "frame_count": 35,
            "frame_step": 0,
            "fps": 14,
            "easing": "linear"
        },
        "uv_size": [38,38],
        "texture": "textures/gui/sprites/container/gif/anvil_hammer"
    },

    /* Enchant */
    "enchant":
    {
        "layer": 2,
        "type": "image",
        "size": [16,16],
        "offset": "$enchant_input_item_loop_offset",
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "uv":
        {
            "anim_type": "flip_book",
            "initial_uv": [0,0],
            "frame_count": 8,
            "frame_step": 0,
            "fps": 8,
            "easing": "linear"
        },
        "uv_size": [16,16],
        "texture": "textures/gui/sprites/container/gif/enchant"
    },

